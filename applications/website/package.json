{
  "private": false,
  "name": "website",
  "version": "0.0.1",
  "description": "magic website demo",
  "main": "./lib/index.js",
  "unpkg": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "precommit.silent": true,
  "pre-commit": [
    "lint"
  ],
  "files": [
    "build",
    "lib",
    "*.js",
    "*.js.map"
  ],
  "scripts": {
    "start": "webpack-dev-server --config .script/webpack.dev.js --hot --inline --progress",
    "https": "HTTPS=true npm start",
    "stage": "rm -rf build && npm run build:umd-stage",
    "build": "rm -rf build && tnpm i && npm run test && npm run build:umd-pro",
    "build:umd-pro": "webpack --config .script/webpack.pro.umd.js --hide-modules --progress --colors",
    "build:umd-stage": "webpack --config .script/webpack.dev.umd.js --hide-modules --progress --colors",
    "build:umdAnalyzer": "HTTPS=true webpack --config .script/webpack.pro.umd.js --hide-modules",
    "build:typings": "tsc -p tsconfig.d.json",
    "lint": "eslint --quiet --ext .tsx,.ts,.js,.jsx src"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  },
  "dependencies": {
    "antd": "^4.8.4",
    "react": "^17.0.1",
    "react-router-dom": "^5.1.2",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@types/lodash": "^4.14.164",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.13.1",
    "core-js": "^3.6.5",
    "css-loader": "^1.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-webpack": "^5.1.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.0.0",
    "less": "^3.0.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pre-commit": "^1.2.2",
    "prettier": "~1.17.0",
    "style-loader": "^2.0.0",
    "typescript": "3.7.3",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^2.5.2",
    "webpackbar": "^5.0.0-3"
  }
}
